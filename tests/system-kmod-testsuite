               on_exit 'modprobe -q -r vport_lisp'modprobe -q vport_stt || echo "Module vport_stt not loaded."
               on_exit 'modprobe -q -r vport_stt'modprobe -q vport_vxlan || echo "Module vport_vxlan not loaded."
               on_exit 'modprobe -q -r vport_vxlan'
   on_exit 'ovs-dpctl del-dp ovs-system'
      touch .conf.db.~lock~
   { set +x
